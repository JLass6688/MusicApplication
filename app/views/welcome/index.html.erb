<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>


<%= form_for @user do |f| %>
	<%= f.label :email %>
	<%= f.text_field :email%>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>


<%= form_tag('sessions', method: :post) do %>
	<%= label_tag :email %>
	<%= text_field_tag :email %>
	<%= label_tag :password %>
	<%= password_field_tag :password %>
	<%= submit_tag %>
<% end %>